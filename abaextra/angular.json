{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aba4": {
      "projectType": "library",
      "root": "projects/aba4",
      "sourceRoot": "projects/aba4/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/aba4/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/aba4/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/aba4/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/aba4/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7e5daa1c-26f7-415b-a919-92e1dbd4ab3a"
  }
}
